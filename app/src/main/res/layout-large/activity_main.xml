<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingStart="5dp"
    android:paddingTop="5dp"
    tools:context=".MainActivity">

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/btnHistory"
        android:layout_width="330dp"
        android:layout_height="68dp"
        android:layout_marginStart="103dp"
        android:layout_marginTop="116dp"
        android:background="@drawable/bookinghistorybtn"
        android:paddingStart="5dp"
        android:paddingTop="5dp"
        android:paddingBottom="5dp"
        android:text="Booking history"
        android:textColor="@color/white"
        android:textSize="28sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView2"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="66dp"
        android:layout_height="52dp"
        android:layout_marginStart="36dp"
        android:layout_marginTop="128dp"
        android:layout_marginEnd="3dp"
        app:layout_constraintEnd_toStartOf="@+id/textView2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/icon" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="192dp"
        android:layout_height="54dp"
        android:layout_marginStart="1dp"
        android:layout_marginTop="134dp"
        android:layout_marginBottom="135dp"
        android:text="Co-working"
        android:textColor="#605c5c"
        android:textSize="34sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/cardbookWorkStation"
        app:layout_constraintStart_toEndOf="@+id/imageView"
        app:layout_constraintTop_toTopOf="parent" />

    <androidx.cardview.widget.CardView
        android:id="@+id/cardbookWorkStation"
        android:layout_width="301dp"
        android:layout_height="296dp"
        android:layout_marginStart="81dp"
        android:layout_marginTop="135dp"
        android:layout_marginEnd="33dp"
        app:cardBackgroundColor="#4d60d1"
        app:cardCornerRadius="14dp"
        app:layout_constraintEnd_toStartOf="@+id/cardMeetingroom"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView2">

        <ImageView
            android:id="@+id/imageView2"
            android:layout_width="177dp"
            android:layout_height="170dp"
            android:layout_gravity="center"
            app:srcCompat="@drawable/cardone" />
    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:id="@+id/cardMeetingroom"
        android:layout_width="301dp"
        android:layout_height="296dp"
        android:layout_marginStart="33dp"
        android:layout_marginTop="134dp"
        android:layout_marginEnd="79dp"
        app:cardBackgroundColor="#c7cffc"
        app:cardCornerRadius="14dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/cardbookWorkStation"
        app:layout_constraintTop_toBottomOf="@+id/btnHistory">

        <ImageView
            android:id="@+id/imageView3"
            android:layout_width="171dp"
            android:layout_height="156dp"
            android:layout_gravity="center"
            app:srcCompat="@drawable/cardtwo" />
    </androidx.cardview.widget.CardView>

    <TextView
        android:id="@+id/textView3"
        android:layout_width="209dp"
        android:layout_height="92dp"
        android:layout_marginTop="16dp"
        android:gravity="center"
        android:text="Book Work  Station"
        android:textSize="34sp"
        app:layout_constraintEnd_toEndOf="@+id/cardbookWorkStation"
        app:layout_constraintHorizontal_bias="0.554"
        app:layout_constraintStart_toStartOf="@+id/cardbookWorkStation"
        app:layout_constraintTop_toBottomOf="@+id/cardbookWorkStation" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="142dp"
        android:layout_height="109dp"
        android:layout_marginTop="12dp"
        android:gravity="center"
        android:text="Meeting  room"
        android:textSize="34sp"
        app:layout_constraintEnd_toEndOf="@+id/cardMeetingroom"
        app:layout_constraintHorizontal_bias="0.553"
        app:layout_constraintStart_toStartOf="@+id/cardMeetingroom"
        app:layout_constraintTop_toBottomOf="@+id/cardMeetingroom" />

</androidx.constraintlayout.widget.ConstraintLayout>